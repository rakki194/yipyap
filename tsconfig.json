{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "./static/dist",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["vite/client"],
    "paths": {
      "~/*": ["./src/*"]
      //     "@solid-primitives/*": [
      //         "/home//repos/solid-primitives/packages/*/src"
      //     ]
    },
    "noEmit": false,
    "declaration": true,
    "isolatedModules": true
  },
  "include": ["src"]
}
